{
    "version": 2,
    "builds": [
        {
            "src": "./vercel_server.py",
            "use": "@vercel/python",
            "config": {
                "pip": ["--requirement", "vercel_requirements.txt"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/vercel_server.py"
        }
    ]
}